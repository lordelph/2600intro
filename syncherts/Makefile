#-----------------------------------------------------------------------------
# default target
#-----------------------------------------------------------------------------

all: syncherts.bin

#-----------------------------------------------------------------------------
# pretty simple...
#-----------------------------------------------------------------------------
syncherts.bin: syncherts.asm
	dasm syncherts.asm -lsyncherts.dump -f3 -v5 -osyncherts.bin

clean:
	rm *.bin
	rm *.dump

.PHONY : all clean

